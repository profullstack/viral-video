# Example environment variables for viral-video-kit
# Copy to .env and edit as needed. Environment variables override config values.

# API keys
OPENAI_API_KEY=
ELEVENLABS_API_KEY=

# Models and voice defaults
TEXT_MODEL=gpt-5
IMAGE_MODEL=gpt-image-1
TTS_MODEL=gpt-4o-mini-tts
TTS_VOICE=alloy

# Video timing defaults
VIDEO_SEC=60
SCENES_COUNT=6

# Development helpers
DRY_RUN=0